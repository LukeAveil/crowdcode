<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :language %>
  <%= f.text_field :language %>
  <%= f.label :description %>
  <%= f.text_area :description, size: "60x12" %>
  <%= f.label :launch_date %>
  <%= f.date_field :launch_date %>
  <%= f.submit %>

<% end %>
